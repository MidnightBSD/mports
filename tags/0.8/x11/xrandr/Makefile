# $MidnightBSD$

PORTNAME=	xrandr
PORTVERSION=	1.4.0
CATEGORIES=	x11

MAINTAINER=	ports@MidnightBSD.org
COMMENT=	Primitive command line interface to the RandR extension
LICENSE=	mit

XORG_CAT=	app
USE_XORG=	xrandr xrender x11

PLIST_FILES=	bin/xrandr \
		bin/xkeystone

MAN1=		xrandr.1

.include <bsd.port.mk>
