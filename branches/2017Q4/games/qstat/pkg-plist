bin/qstat
@unexec if cmp -s %D/etc/qstat.cfg %D/etc/qstat.cfg.default; then rm -f %D/etc/qstat.cfg; fi
etc/qstat.cfg.default
@exec [ -f %B/qstat.cfg ] || cp %B/%f %B/qstat.cfg
